[push]
	default = simple
[pull]
	rebase = true
	stat = true
[alias]
	blog = log --decorate --oneline
	flog = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --full-history --sparse
	slog = log --pretty=format:"%h%x09%an%x09%ad%x09%s" --date=local 
[credential]
	helper = !aws codecommit --profile default --region us-east-1 credential-helper $@
	usehttppath = true
[remote "origin"]
	prune = true
[merge]
	stat = true
[user]
	email = matt.jeffery@arkansas.gov
	name = Matt Jeffery
